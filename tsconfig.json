{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@/components/*": ["./src/components/*"],
			"@/lib/*": ["./src/lib/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		]
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"old/api/hello.ts",
		"pages/hello.js",
		"old/api/trigger.js",
		".next/types/**/*.ts",
		"src/components/Client/adminpage/Loggin.js",
		"src/app/_login/page.js",
		"src/components/Client/shadcn/table/RenderProductTable.js",
		"src/components/Client/shadcn/table/ProductTable/columns.js",
		"src/app/loggedin/layout.js",
		"src/components/Client/Pages/produkte/columns.js",
		"src/app/loggedin/produkte-tabelle/page.js",
		"src/components/Client/Pages/produkte/data-table.js",
		"src/app/page.js",
		"src/app/api/instagramproxy/route.js"
	],
	"exclude": ["node_modules"]
}
